#include "sim_objects/default_trick_sys.sm"

##include "model/model.hpp"

    class ModelSimObject : public Trick::SimObject {
public:
  // Required for variable server.
  Model model;

  ModelSimObject() {
    ("initialization") model.initialization();
    (1.0, "scheduled") model.scheduled();
  }
};

ModelSimObject sim;